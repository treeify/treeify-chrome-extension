    "highlight.js": "^11.4.0",
    "katex": "^0.15.2",
    "mnemonist": "^0.39.0",
    "nanoid": "^3.3.1",
    "object-path": "^0.11.8",
    "power-radix": "^2.5.6",
    "ua-parser-js": "^1.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.1",
