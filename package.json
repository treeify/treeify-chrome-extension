    "@types/power-radix": "^2.5.0",
    "@types/ua-parser-js": "^0.7.36",
    "cross-env": "^7.0.3",
    "npm-check-updates": "^12.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
